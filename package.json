{
  "name": "commonly.typescript",
  "version": "0.2.0",
  "description": "Well typed sorted utilities commonly desired.",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && tsc -p tsconfig-cjs.json && cp package.json , *readme.md ./dist",
    "test": "jest",
    "lint": "eslint --ext .ts ."
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "author": "Miguel Leon",
  "license": "ISC",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./cjs/index.js"
    },
    "./*": {
      "import": "./*/index.js",
      "require": "./cjs/*/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/miguel-leon/commonly.typescript"
  },
  "keywords": ["typescript", "util"]
}
