{
  "name": "utils.typescript",
  "version": "0.0.1",
  "description": "Utils Typescript",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc && tsc -p tsconfig-cjs.json && cp package.json ./dist",
    "test": "jest",
    "lint": "eslint --ext .ts ."
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.5",
    "typescript": "^4.3.5"
  },
  "author": "Miguel Leon",
  "license": "ISC",
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./cjs/index.js"
    },
    "./*": {
      "import": "./*/index.js",
      "require": "./cjs/*/index.js"
    }
  }
}
